public class Enemy {


	protected int x;
	protected int y;
	public static final EnemyImage texture = EnemyImage.monster;
	
	public Enemy() {
		x = 0;
		y = 0;
	}
	
	public Enemy(int x, int y) {
		this.x = x;
		this.y = y;
	}

	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}
	
	
	
}
